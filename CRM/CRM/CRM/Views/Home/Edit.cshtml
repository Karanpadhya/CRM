@model CRM.Models.Profile

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<!DOCTYPE html>

<html>
<head>
    <title>Edit your Response</title>
</head>
<body>
<div class="row">
    <br />
    <br />
    <table style="width: 50%" border="2px" align="left">
        <tr>
            <th>@Html.LabelFor(a => a.Fname)</th>


            <td>@Html.TextBoxFor(a => a.Fname)</td>
        </tr>
        <tr>
            <th>@Html.LabelFor(a => a.Lname)</th>

            <td>@Html.TextBoxFor(a => a.Lname)</td>
        </tr>
        <tr>
            <th>@Html.LabelFor(a => a.Phonenumber)</th>

            <td>@Html.TextBoxFor(a => a.Phonenumber)</td>
        </tr>
        <tr>
            <th>@Html.LabelFor(a => a.Emailaddress)</th>

            <td>@Html.TextBoxFor(a => a.Emailaddress)</td>
        </tr>


    </table>

       
</div>
<br/>
<button id="btnsave">Save</button>
<script type="text/javascript">
    document.getElementById("btnsave").onclick = function () {
        //logic to save in DB
        location.href = "../Index";
    };
</script>

<button id="myButton" >Home</button>

<script type="text/javascript">
    document.getElementById("myButton").onclick = function () {
        
        location.href = "../Index";
    };
</script>

</body>
</html>


